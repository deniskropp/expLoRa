{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [ "eslint:recommended", "next/core-web-vitals" ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}
